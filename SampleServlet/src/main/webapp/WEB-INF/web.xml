<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:jsp="http://java.sun.com/xml/ns/javaee/jsp" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <jsp-config>
    <jsp-property-group>
      <url-pattern>*.jsp</url-pattern>
      <page-encoding>UTF-8</page-encoding>
      <scripting-invalid>true</scripting-invalid>
    </jsp-property-group>
  </jsp-config>
  <servlet>
    <description></description>
    <display-name>Runner3</display-name>
    <servlet-name>Runner3</servlet-name>
    <servlet-class>tech.csm.SampleServlet.Runner3</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Runner3</servlet-name>
    <url-pattern>/Runner3</url-pattern>
  </servlet-mapping>
</web-app>